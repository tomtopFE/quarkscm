var TT_NS = (function(NS, $){
    var $menu_li = $(".nav_l li");
    $menu_li.mouseenter(function() {
        $(this).addClass("cur");
        $(this).children("div.slide_menu").show();
    })
    $menu_li.mouseleave(function(){
        $(this).removeClass("cur");
        $(this).children("div.slide_menu").hide();
    })
})(window.TT_NS || {}, jQuery);

